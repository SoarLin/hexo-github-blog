<head>
  <meta charset="utf-8">
  <meta name="google-site-verification" content="PG3_4p9inh6jJ-LnJOEhbVkRTHhjlg9nOy0sDl8WVJA" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="<%= config.description %>">
  <meta name="keyword"  content="<%= config.keyword %>">
  <meta name="theme-color" content="#600090">
  <meta name="msapplication-navbutton-color" content="#600090">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="#60090">
  <% if (config.favicon) { %>
    <link rel="icon" type="image/png" href="<%= config.favicon %>">
  <% } %>
  <% if (is_post() && config.generator_amp){ %>
    <link rel="amphtml" href="./amp/index.html">
  <% } %>
  <link rel="shortcut icon" href="<%= config.shortcut_icon %>">
  <link rel="alternate" type="application/atom+xml" title="<%= config.title %>" href="/atom.xml">

  <title>
    <% if (page.title) { %>
      <%= page.title + 'ï½œ' + config.SEOTitle %>
    <% } else { %>
      <%= config.SEOTitle %>
    <% } %>
  </title>

  <link rel="canonical" href="<%= config.url + config.root + (page.path||'').replace('index.html', '') %>">

  <!-- Bootstrap Core CSS -->
  <link rel="stylesheet" href="/css/bootstrap.min.css" media="none" onload="if(media!='all')media='all'">
  <!-- Custom CSS -->
  <link rel="stylesheet" href="/css/blog-style.css" media="none" onload="if(media!='all')media='all'">
  <!-- FontsAwesome -->
  <link rel="stylesheet" href="/css/font-awesome.min.css" media="none" onload="if(media!='all')media='all'">

  <noscript>
      <link rel="stylesheet" href="/css/bootstrap.min.css">
      <link rel="stylesheet" href="/css/blog-style.css">
      <link rel="stylesheet" href="/css/font-awesome.min.css">
  </noscript>

  <!-- Pygments Github CSS -->
